    <selector
        xmlns:android="http://schemas.android.com/apk/res/android">
        <item android:state_pressed="true">
            <shape android:shape="rectangle">
                <solid android:color="#8CBF4D" />
                <corners android:radius="5dp"/>
                <stroke android:width="1.0dip" android:color="#8CBF4A" />

            </shape>
        </item>
        <item>
            <shape android:shape="rectangle">
                <solid android:color="#8CBF4D" />
                <stroke android:width="1.0dip" android:color="#8CBF4C" />
                <corners android:radius="5dp"/>
            </shape>
        </item>
    </selector>
